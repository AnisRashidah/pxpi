[Unit]
Description=MAVLink Router
Wants=zerotier-one.service
After=network-online.target zerotier-one.service

[Service]
Type=forking
PIDFile=/var/run/mavlink-routerd.pid
ExecStart=/usr/bin/mavlink-routerd -e 192.168.192.255:14550 -e 127.0.0.1:14550
Restart=on-failure
RestartSec=3

[Install]
WantedBy=multi-user.target
