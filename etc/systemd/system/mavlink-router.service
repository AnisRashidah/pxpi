[Unit]
Description=MAVLink Router
Wants=zerotier-one.service
After=network-online.target zerotier-one.service

[Service]
Type=simple
# PIDFile=/var/run/mavlink-routerd.pid
ExecStart=/home/pi/mavlink-router-service/mavlink-router.sh
